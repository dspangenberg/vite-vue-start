{
    "extends": [
        "plugin:vue/vue3-recommended",
        "standard"
    ],
    "plugins": [
        "vue"
    ],
    "env": {
        "browser": true,
        "es2021": true,
        "vue/setup-compiler-macros": true
    },
    "rules": {
        "no-console": "off",
        "no-unused-vars": "warn",
        "vue/no-multiple-template-root": "off",
        "vue/no-unused-components": "warn",
        "vue/script-setup-uses-vars": "error",
        "vue/multi-word-component-names": "off"
    }
}